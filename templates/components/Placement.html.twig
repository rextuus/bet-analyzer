<div class="placement" {{ attributes }}>
    <tr class="{{ this.rowClass }}">
        <td>{{ placement.created|date() }}</td>
        <td class="{{ this.homeClass }}">{{ placement.fixtures[0].homeTeamName }}</td>
        <td>
            <div class="game-result">
                <div class="icon-container">
                    <img class="icon home-win" src="{{ asset(this.getAssetPath) }}" alt="Home Win Icon">
                </div>
            </div>
        </td>
        <td class="{{ this.awayClass }}">{{ placement.fixtures[0].awayTeamName }}</td>
        <td>
            <table>
                <tr>
                    <td>3-Way</td>
                    <td class="{{ this.homeClass }}">{{ placement.fixtures[0].oddHome }}</td>
                    <td class="{{ this.drawClass }}">{{ placement.fixtures[0].oddDraw }}</td>
                    <td class="{{ this.awayClass }}">{{ placement.fixtures[0].oddAway }}</td>
                </tr>
                <tr>
                    <td>Over/Under</td>
                    <td></td>
                    <td class="{{ this.homeClass }}">{{ this.OverOdd }}</td>
                    <td class="{{ this.awayClass }}">{{ this.UnderOdd }}</td>
                </tr>
                <tr>
                    <td>Both Score</td>
                    <td></td>
                    <td class="{{ this.homeClass }}">{{ this.BothTeamsScoreOdd }}</td>
                    <td class="{{ this.awayClass }}">{{ this.BothTeamsScoreNotOdd }}</td>
                </tr>
            </table>
        </td>
        <td>{{ placement.fixtures[0].result.value }}</td>
        <td>
            <div class="transaction {{ this.rowClass }}">
                <div class="icon">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24">
                        {{ this.iconPath|raw }}
                    </svg>
                </div>
                <div class="amount">{{this.outPut}}</div>
            </div>
            {{ this.newCashBox }}
        </td>
        <td><div class="result">{{  placement.fixtures[0].endScoreHome }} - {{ placement.fixtures[0].endScoreAway }}</div></td>
        <td><a href="https://www.google.com/search?q={{ placement.fixtures[0].homeTeamName }} - {{ placement.fixtures[0].homeTeamName }} {{ placement.created|date() }} result" target="_blank">Google</a></td>
    </tr>
</div>
